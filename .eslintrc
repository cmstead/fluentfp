{
    "extends": "eslint:recommended",
    "parser": "esprima",
    "env": {
        "browser": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {}
    },
    "rules": {
        "no-undef": 1,
        "no-console": 1
    },
    "globals": {
        "describe": false,
        "it": false,
        "before": false,
        "after": false,
        "beforeEach": false,
        "afterEach": false,
        "signet": false,
        
        "coreAppendable": false,
        "coreFunctions": false,
        "coreMappable": false,
        "coreMonads": false,
        "corePredicates": false,
        "coreTransformable": false,
        "coreTypes": false,
        "fluentCore": false,
        "fluentArray": false
    }
}
